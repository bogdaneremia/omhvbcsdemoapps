{
    "applicationModelVersion": "19.1.3",
    "id": "starter",
    "description": "A new VB app",
    "defaultPage": "app",
    "chains": {},
    "services": {},
    "translations": {
        "app": {
            "path": "./resources/strings/app/nls/app-strings"
        }
    },
    "types": {},
    "variables": {
        "username": {
            "type": "string"
        },
        "password": {
            "type": "string",
            "persisted": "none"
        },
        "authenticated": {
            "type": "boolean",
            "defaultValue": false,
            "persisted": "session"
        },
        "loggedUserDisplayName": {
            "type": "string",
            "defaultValue": "Anonymous",
            "persisted": "session"
        }
      },
    "userConfig": {
        "type": "vb/DefaultSecurityProvider",
        "configuration": {}
    },
    "security": {
        "access": {
            "requiresAuthentication": false
        }
      }
}